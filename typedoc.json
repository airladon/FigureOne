{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./src/js"],
  "entryPointStrategy": "expand",
  "out": "./docs/api-typedoc",
  "excludeNotDocumented": true,
  "excludePrivate": true,
  "excludeInternal": true,
  "sort": ["source-order"],
  "groupOrder": [
    "Introduction",
    "Geometry",
    "Figure",
    "Figure Elements",
    "2D Shape Primitives",
    "Text",
    "3D Shape Primitives",
    "2D Shape Collections",
    "3D Shape Collections",
    "Interactivity",
    "Animation",
    "Equations",
    "Equation Layout",
    "Equation Symbols",
    "Morphing",
    "Slide Navigator",
    "Interactive Video",
    "Shaders",
    "2D Geometry Tools",
    "Geometry Creation",
    "Math Tools",
    "Misc Figure Element",
    "Misc Geometry",
    "Misc Shapes",
    "Misc Geometry Creation",
    "Misc Text",
    "Misc Animation",
    "Misc Equation",
    "Misc Morphing",
    "Misc SlideNavigator",
    "*"
  ],
  "navigation": {
    "includeGroups": true
  },
  "projectDocuments": [
    "./docs/api/tutorials/intro.md",
    "./docs/api/tutorials/geometric/geometric.md",
    "./docs/api/tutorials/figure/figure.md",
    "./docs/api/tutorials/draw/draw.md",
    "./docs/api/tutorials/2d-shape-primitives-api.md",
    "./docs/api/tutorials/text/text.md",
    "./docs/api/tutorials/shapes3d/shapes3d.md",
    "./docs/api/tutorials/collections/collections.md",
    "./docs/api/tutorials/interactive/interactive.md",
    "./docs/api/tutorials/animation/animation.md",
    "./docs/api/tutorials/equation/equation.md",
    "./docs/api/tutorials/morph/morph.md",
    "./docs/api/tutorials/recorder/recorder.md",
    "./docs/api/tutorials/shaders/shaders.md"
  ],
  "plugin": ["typedoc-plugin-mdn-links", "./typedoc-strip-prefix.cjs"],
  "customCss": "./docs/api/typedoc-custom.css"
}
