{
  "name": "figureone",
  "version": "1.0.3",
  "description": "Draw, animate and interact with shapes, text, plots and equations in Javascript. Create interactive slide shows, and interactive videos.",
  "main": "index.js",
  "types": "types/index.d.ts",
  "unpkg": "figureone.min.js",
  "jsdelivr": "figureone.min.js",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "default": "./index.js"
    }
  },
  "files": [
    "index.js",
    "figureone.min.js",
    "types/",
    "llms.txt",
    "llms-full.txt"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/airladon/figureone.git"
  },
  "scripts": {
    "browser": "./browser.sh",
    "webpack": "webpack",
    "build:types": "tsc --project tsconfig.build.json",
    "build": "webpack && npm run build:types",
    "tsc": "tsc --noEmit",
    "jest": "jest",
    "jest-perf": "jest --runInBand --config jest.perf.config.js",
    "jest-perf-debug": "node --inspect-brk=0.0.0.0 node_modules/.bin/jest --runInBand --config jest.perf.config.js",
    "lint": "eslint src",
    "jest-debug": "node --inspect-brk=0.0.0.0 node_modules/.bin/jest --runInBand",
    "docs:typedoc": "bash generate_docs_typedoc.sh"
  },
  "keywords": [
    "interactive",
    "figure",
    "webgl",
    "equations",
    "animation"
  ],
  "author": "airladon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/airladon/figureone/issues"
  },
  "homepage": "https://github.com/airladon/figureone#readme",
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/compat": "^1.0.0",
    "@eslint/eslintrc": "^3.0.0",
    "@eslint/js": "^10.0.0",
    "babel-jest": "^30.0.0",
    "babel-loader": "^10.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "documentation": "^14.0.0",
    "eslint": "^10.0.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^29.0.0",
    "globals": "^16.0.0",
    "http-server": "^14.0.0",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^30.0.0",
    "regenerator-runtime": "^0.14.0",
    "typedoc": "^0.28.17",
    "typedoc-plugin-mdn-links": "^5.1.1",
    "webpack": "^5.58.2",
    "webpack-cli": "^6.0.0",
    "whatwg-fetch": "^3.6.2",
    "worker-loader": "^3.0.8",
    "typescript": "^5.4.2"
  },
  "overrides": {
    "eslint": "$eslint",
    "eslint-config-airbnb-base": {
      "eslint": "$eslint"
    },
    "eslint-plugin-import": {
      "eslint": "$eslint"
    },
    "eslint-plugin-jest": {
      "eslint": "$eslint"
    },
    "@eslint/compat": {
      "eslint": "$eslint"
    },
    "@eslint/eslintrc": {
      "eslint": "$eslint"
    }
  },
  "devDependenciesComments": {
    "worker-loader": "Do not upgrade to 3.0.0 as the publicPath options seems to be broken. This means instead of workers being searched for at /static/workers, they are searched for at the location of the file calling them"
  }
}
